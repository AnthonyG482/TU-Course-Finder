{
  "manifest_version": 3,
  "name": "TU Course Finder",
  "version": "0.01",
  "description": "A Chrome Extension to help you locate your courses",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://csprd.towson.edu/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
